<!DOCTYPE html>
<html lang="{{ craft.app.language }}">
  <head>
    <meta content="IE=edge" http-equiv="X-UA-Compatible">
    <meta charset="utf-8"/>
    <title>{{ siteName }}</title>
    <meta content="width=device-width, initial-scale=1.0, maximum-scale=1.0, user-scalable=no, viewport-fit=cover" name="viewport">
    <link rel="stylesheet" type="text/css" href="/web/assets/scripts/styles.css">
    <script src="https://code.jquery.com/jquery-2.2.0.min.js" type="text/javascript"></script>
    <script src="https://cdnjs.cloudflare.com/ajax/libs/slick-carousel/1.6.0/slick.js"></script>
    <link rel="stylesheet" href="https://use.typekit.net/fyu1tcm.css">
    <link rel="stylesheet" href="https://stackpath.bootstrapcdn.com/bootstrap/4.5.2/css/bootstrap.min.css" integrity="sha384-JcKb8q3iqJ61gNV9KGb8thSsNjpSL0n8PARn9HuZOnIxN0hoP+VmmDGMN5t9UJ0Z" crossorigin="anonymous">
    <script src="main.js" type="text/javascript"></script>
    <link rel="stylesheet" href="/web/assets/scripts/animate.min.css" />
  <body>
      {% block content %}
      {% endblock %}

    <footer>
    <!-- Landing page FOOTER -->
    <div id="landingpage-footer" class="animate__animated animate__slideInLeft">
        <div id="landingpage-footer-section" class="section-grad ">
            <div class="row">
                <div class="landingpage-footer-container animate__delay-1s">
                    
                    <!-- LP footer left-side LOGO -->
                    <div class="left-lp-footer-col">
                        <a class="lp-footer-logo animate__animated animate__zoomIn animate__delay-1s" href=""><img src="/web/assets/landingpages/Fulcrum Premium Finance - Footer logo.svg" /></a>
                        <p class="lp-footer-text animate__animated animate__zoomIn animate__delay-1s">Â© FULCRUM PREMIUM FINANCE 2020. All rights reserved.</p>
                    </div>

                    <!-- LP footer right-side SOCIAL MEDIA ICONS -->
                    <div class="right-lp-footer-col animate__animated animate__zoomIn animate__delay-1s">

                        <div class="right-lp-footer-col-heading-container">
                            <p class="follow-us animate__animated animate__zoomIn animate__delay-1s">FOLLOW US</p>
                        </div>

                        <div class="footer-social-media-icons-container">
                            <a id="footer-instagram" class="footer-social-icon animate__animated animate__zoomIn"><img src="/web/assets/landingpages/instagram-footer-icon.svg"></a>
                            <a id="footer-facebook" class="footer-social-icon animate__animated animate__zoomIn"><img src="/web/assets/landingpages/facebook-footer-icon.svg"></a>
                            <a id="footer-twitter" class="footer-social-icon animate__animated animate__zoomIn"><img src="/web/assets/landingpages/twitter-footer-icon.svg"></a>
                            <a id="footer-youtube" class="footer-social-icon animate__animated animate__zoomIn"><img src="/web/assets/landingpages/youtube-footer-icon.svg"></a>
                            <a id="footer-linkedin" class="footer-social-icon animate__animated animate__zoomIn"><img src="/web/assets/landingpages/linkedin-footer-icon.svg"></a>
                        </div>

                    </div>

                </div>
            </div>
        </div>
    </div>
    </footer>
  </body>
</html>